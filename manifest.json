{
  "name": "elunico URL Shortener",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension lets you create short urls using url.eluni.co",
  "homepage_url": "https://github.com/elunico",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "URL Shortener",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "clipboardWrite",
    "tabs",
    "https://*/*",
    "http://url.eluni.co/create"
  ]
}
